{
  "version": 2,
  "builds": [
    {
      "src": "analyze.js",
      "use": "@vercel/node",
      "config": {
        "region": "hkg1" 
      }
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/analyze",
      "dest": "/analyze.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
